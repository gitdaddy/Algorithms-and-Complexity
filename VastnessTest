
      cerr << "Size of pow2:" << pow2 << " = " << sizeN
           << " Size of Prefix:" << prefix << " = " << sizeP << endl;

      twoNArray = covertToArray(pow2, sizeN);


      if(sizeN >= sizeP) // Only test if the prefix is equal to or a subset
      {
          if(testPrefix(twoNArray, pArray, sizeP))
          {
             cerr << "solution: 2^" << n << "\n";
           break;
         }
      }
      m++;
   }

   return 0;
}
